
<view class="container">
    
    <view class="search-bar">
        <view class="weui-search-bar">
            <view class="weui-search-bar__form">
                <view class="weui-search-bar__box">
                    <icon type="search" size="14" class="weui-icon-search_in-box"></icon>
                    <input type="text" placeholder="搜索" value="{{ inputVal }}" focus="{{ inputShowed }}" data-wpy-evt="5-0" bindblur="_proxy" bindinput="_proxy" class="weui-search-bar__input"></input>
                    <view wx:if="{{ inputVal.length > 0 }}" data-wpy-evt="5-2" bindtap="_proxy" class="weui-icon-clear">
                        <icon type="clear" size="14"></icon>
                    </view>
                </view>
                <label hidden="{{ inputShowed }}" data-wpy-evt="5-3" bindtap="_proxy" class="weui-search-bar__label">
                    <icon type="search" size="14" class="weui-icon-search"></icon>
                    <view class="weui-search-bar__text">搜索</view>
                </label>
            </view>
            <view hidden="{{ !inputShowed }}" data-wpy-evt="5-4" bindtap="_proxy" class="weui-search-bar__cancel-btn">取消</view>
        </view>
    </view>
    
    <view class="navbar">
        <view class="item active">全部活动全部活动全部活动</view>
        <view class="item">热门活动</view>
        <view class="item">最新活动</view>
    </view>
    
