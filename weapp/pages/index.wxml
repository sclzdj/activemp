
<view class="container">
    
    <view class="search-bar">
        <view class="weui-search-bar">
            <view class="weui-search-bar__form">
                <view class="weui-search-bar__box">
                    <icon type="search" size="14" class="weui-icon-search_in-box"></icon>
                    <input type="text" placeholder="搜索" value="{{ inputVal }}" focus="{{ inputShowed }}" data-wpy-evt="4-65" bindblur="_proxy" bindinput="_proxy" class="weui-search-bar__input"></input>
                    <view wx:if="{{ inputVal.length > 0 }}" data-wpy-evt="4-67" bindtap="_proxy" class="weui-icon-clear">
                        <icon type="clear" size="14"></icon>
                    </view>
                </view>
                <label hidden="{{ inputShowed }}" data-wpy-evt="4-68" bindtap="_proxy" class="weui-search-bar__label">
                    <icon type="search" size="14" class="weui-icon-search"></icon>
                    <view class="weui-search-bar__text">搜索</view>
                </label>
            </view>
            <view hidden="{{ !inputShowed }}" data-wpy-evt="4-69" bindtap="_proxy" class="weui-search-bar__cancel-btn">取消</view>
        </view>
    </view>
    
    <view class="swiper">
        <swiper indicator-dots="true" autoplay="true" interval="3000" duration="500">
            <block wx:for="{{ imgUrls }}" wx:for-index="index" wx:for-item="item" wx:key="index">
